<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MateoSotomayorProgreso2.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MateoSotomayorProgreso2"
    Shell.FlyoutBehavior="Flyout"
    Title="MateoSotomayorProgreso2">

    <TabBar>
        <Tab Title="Recarga">
            <ShellContent
            Title="lo"
            ContentTemplate="{DataTemplate local:Recarga}"
            Route="Recarga"/>
        </Tab>
        <Tab Title="Grid">
            <ShellContent
             Title="ww"
             ContentTemplate="{DataTemplate local:Grid}"
             Route="Grid"/>
        </Tab>

    </TabBar>
</Shell>
